  "version": "0.2.0",
  "configurations": [
  {
    "name": "XDebug php",
    "type": "php",
    "request": "launch",
    "port": 9000
  },
  {
    "name": "Проверить php скрипт",
    "type": "php",
    "request": "launch",
    "program": "${file}",
    "cwd": "${fileDirname}",
    "port": 9000
  },
  {
    "type": "node",
    "request": "launch",
    "name": "Gulp ",
    "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
    "autoAttachChildProcesses": true,
    "console": "integratedTerminal",
    "args": [
      "default"
    ],
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  
   
  ]
